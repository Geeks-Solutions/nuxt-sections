{
  "name": "@geeks.solutions/nuxt-sections",
  "version": "3.0.3",
  "author": "Geeks Solutions info@geeks.solutions (https://www.geeks.solutions)",
  "repository": {
    "type": "git",
    "url": "https://github.com/Geeks-Solutions/nuxt-sections.git"
  },
  "keywords": [
    "page speed",
    "ssr render",
    "page building",
    "A/B testing",
    "Content authoring"
  ],
  "description": "A NuxtJS module to interface with Sections Serverless Page Building and rendering engine",
  "license": "ISC",
  "homepage": "https://sections.geeks.solutions",
  "contributors": [],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit",
    "link": "npm link"
  },
  "dependencies": {
    "@nuxt/kit": "^3.17.1",
    "@nuxtjs/i18n": "^9.5.3",
    "intro.js": "^7.2.0",
    "vue-toastification": "^2.0.0-rc.5",
    "@marshallswain/vuedraggable": "^4.2.0",
    "@geeks.solutions/vue-components": "3.0.1"
  },
  "devDependencies": {
    "@nuxt/devtools": "^2.4.0",
    "@nuxt/eslint": "^1.3.0",
    "@nuxt/eslint-config": "^1.3.0",
    "@nuxt/module-builder": "^1.0.1",
    "@nuxt/schema": "^3.17.1",
    "@nuxt/test-utils": "^3.17.2",
    "@testing-library/vue": "^8.1.0",
    "@types/node": "latest",
    "@vue/test-utils": "^2.4.6",
    "changelogen": "^0.6.1",
    "eslint": "^9.25.1",
    "happy-dom": "^17.4.6",
    "jsdom": "^26.1.0",
    "nuxt": "^3.17.1",
    "typescript": "~5.8.3",
    "vitest": "^3.1.2",
    "vue-tsc": "^2.2.10"
  },
  "peerDependencies": {
    "vue-toastification": "^2.0.0-rc.5",
    "@marshallswain/vuedraggable": "^4.2.0",
    "@pinia/nuxt": "^0.11.0",
    "pinia": "^3.0.2"
  }
}
